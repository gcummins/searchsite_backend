#confirm_delete_div
{
	width: 400px;
	border-top: 6px solid #ababab;
	border-left: 6px solid #ababab;
	border-right: 6px solid #5c5959;
	border-bottom: 6px solid #5c5959;
	top: 150px;
	left: 40%;
	position: fixed;
	background: gray;
	filter:alpha(opacity=95);-moz-opacity:.95;opacity:.95;
	padding: 0px 0px 10px 5px;
	display: none;
}
#confirm_delete_image_div
{
	float: left;
	width: 135px;
	height: 135px;
}
#confirm_delete_image_div img
{
	margin: 10px 10px 5px 5px;
}
#edit_div
{
	width: 500px;
	border-top: 6px solid #ababab;
	border-left: 6px solid #ababab;
	border-right: 6px solid #5c5959;
	border-bottom: 6px solid #5c5959;
	top: 150px;
	left: 38%;
	position: fixed;
	background: gray;
	filter:alpha(opacity=95);-moz-opacity:.95;opacity:.95;
	padding: 0px 0px 10px 5px;
	display: none;
}
#edit_div table td.detail_cell label
{
	display: block;
	width: 155px;
	float: left;
	text-align: right;
	padding-right: 10px;
}
#edit_div table td.detail_cell input
{
	margin: 0px 5px 10px 0px;
}
#edit_div table td.detail_cell input.radiobutton
{
	margin-bottom: 0px;
}
#edit_div td.detail_cell input.submitbutton
{
	float: right;
}
ul#legend
{
	width: 100%;
}
ul#legend li
{
	margin-right: 10px;
}
ul#legend li span.legend_ad_disabled
{
	background-color: #c9c2c2;
}
ul#legend li span.legend_ad_active
{
	color: green;
}
ul#legend li span.legend_ad_expired
{
	color: red;
}
ul#legend li span.legend_ad_upcoming
{
	color: #00ceff;
}
#order_select_cell
{
	display: none;
}
#order_select_div
{
	float: right;
}
#order_select
{
	width: 155px;
}
#order_select optgroup
{
	font-size: 125px;
	background: no-repeat top left;
	background-color: #efefef;
	border: 1px solid blue;
	margin-bottom: 5px;
}
#order_select optgroup option
{
	font-size: 40px;
}